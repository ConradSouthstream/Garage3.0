@model Garage3.Models.Vehicle

@{
    ViewData["Title"] = "Create";
}

<div class="modal-content">
    <h1><strong>Welcome to Garage Inc.</strong></h1>
    <h5>Please enter your license number.</h5>

    <form asp-action="ParkingProcess">
        <div class="form-group">
            <label asp-for="LicenseNumber" class="control-label"></label>
            <input asp-for="LicenseNumber" class="form-control" />
            <span asp-validation-for="LicenseNumber" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Park" class="btn btn-primary" />
        </div>
    </form>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
